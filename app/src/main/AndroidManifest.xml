<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.controlemensalidades">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icone"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icone"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AlunoMensalidadeActivity"></activity>
        <activity android:name=".SobreActivity" />
        <activity android:name=".CadastroMensalidadesActivity" />
        <activity android:name=".ListMensalidadesActivity" />
        <activity android:name=".CadastroAlunosActivity" />
        <activity android:name=".ListAlunosActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>